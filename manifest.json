{
  "manifest_version": 3,
  "name": "Crisp Checker",
  "version": "1.4.1",
  "description": "Calculate and display the maximum allowed binder.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
