{
    "manifest_version": 3,
    "name": "Crisp Checker 0.01",
    "version": "0.01",
    "description": "Rebuilding this extension",
    "permissions": ["activeTab", "scripting", "tabs", "storage"],
    "host_permissions": [
      "https://camel.hellomoving.com/*"
    ],
    "action": {
      "default_title": "Crisp Check Extension",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "background": {
      "service_worker": "js/background.js"
    },
    "browser_action": {
      "default_title": "Crisp Check Extension",
      "default_popup": "html/popup.html"
    }
}